{
  "name": "boilerplate",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/neloreck/x-core-react-boilerplate",
  "description": "X-core react boilerplate project.",
  "main": "./src/application/Application.ts",
  "scripts": {
    "setup": "npx lerna bootstrap",
    "clean": "npm run cli purge && npx lerna clean -y ",
    "===============================================================": "===============================================",
    "start": "npx cross-env NODE_ENV=production npm run cli start",
    "start:dev": "npx cross-env NODE_ENV=development npm run cli start:dev",
    "=================================================================": "============================================",
    "build": "npx cross-env NODE_ENV=production npm run cli build",
    "build:dev": "npx cross-env NODE_ENV=development npm run cli build",
    "================================================================": "=============================================",
    "cli": "npx ts-node --project ./cli/tsconfig.json ./cli/CliRunner.ts",
    "test": "npm run cli test"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "author": "Igor Syrotenko",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.6.0",
    "core-js": "^3.2.1",
    "fast-async": "^6.3.8",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
