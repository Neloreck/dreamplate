{
  "compilerOptions": {
    "strict": true,
    "emitDecoratorMetadata": false,
    "esModuleInterop": false,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "module": "es6",
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "dom"
    ],
    "baseUrl": "./",
    "outDir": "../target/dist:ts",
    "paths": {
      "@Build/*": [ "../cli/build/*" ],
      "@Macro/*": [ "../cli/build/macroses/*" ],
      "@Test/*": [ "../cli/test/*" ],
      "@Application/*": [ "./application/*" ],
      "@Core": [ "./application/core/index" ],
      "@Core/*": [ "./application/core/*" ],
      "@Modules/*": [ "./application/modules/*" ],
      "@Modules": [ "./application/modules/index" ],
      "@Api/*": [ "./api/*" ],
      "@Lib/*": [ "./lib/*" ]
    },
    "resolveJsonModule": true
  },
  "types": [
    "node"
  ],
  "exclude": []
}
