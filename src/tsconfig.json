{
  "compilerOptions": {
    "strict": true,
    "emitDecoratorMetadata": false,
    "esModuleInterop": false,
    "experimentalDecorators": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "module": "es6",
    "inlineSources": false,
    "tsBuildInfoFile": "../target/cache/incremental",
    "incremental": true,
    "target": "esnext",
    "moduleResolution": "node",
    "jsx": "preserve",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "dom"
    ],
    "baseUrl": "./",
    "outDir": "../target/dist:raw",
    "paths": {
      "@Build/*": [ "../cli/build/*" ],
      "@Macro/*": [ "../cli/build/macroses/*" ],
      "@Test/*": [ "../cli/test/*" ],
      "@Application/*": [ "./application/*" ],
      "@Main": [ "./application/main/index" ],
      "@Main/*": [ "./application/main/*" ],
      "@Modules/*": [ "./application/modules/*" ],
      "@Modules": [ "./application/modules/index" ],
      "@Api/*": [ "./api/*" ],
      "@Lib/*": [ "./lib/*" ]
    },
    "resolveJsonModule": true
  },
  "types": [
    "node"
  ],
  "exclude": [],
  "typedocOptions": {
    "out": "../docs",
    "exclude": [ "**/node_modules/**", "**/cli/**", "**/*.test.*", "**/index*", "/**/__tests__/**" ],
    "mode": "modules",
    "name": "Dreamplate"
  }
}
