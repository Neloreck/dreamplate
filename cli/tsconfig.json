{
  "extends": "../src/tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "esModuleInterop": true,
    "suppressImplicitAnyIndexErrors": true,
    "resolveJsonModule": true,
    "module": "commonjs",
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2018",
      "dom"
    ],
    "incremental": true,
    "baseUrl": "./",
    "paths": {
      "#/*": [ "./*" ],
    }
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
